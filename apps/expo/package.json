{
  "name": "@planty/expo",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "clean": "git clean -xdf .expo .turbo node_modules",
    "prebuild": "cross-env pnpm expo prebuild",
    "dev": "REACT_NATIVE_PACKAGER_HOSTNAME=$(netsh.exe interface ip show address 'Wi-Fi 2' | grep -a '192.168.1.11' | grep -oP '\\d+\\.\\d+\\.\\d+\\.\\d+') expo start",
    "dev:android": "expo start --android",
    "dev:ios": "expo start --ios",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "format": "prettier --debug-check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "install-maestro": "curl -Ls 'https://get.maestro.mobile.dev' | bash",
    "test": "jest",
    "e2e-test": "maestro test .maestro/ -e APP_ID=com.corentinkittel.planty",
    "start:staging": "cross-env APP_ENV=staging pnpm run start",
    "prebuild:staging": "cross-env APP_ENV=staging pnpm run prebuild",
    "android:staging": "cross-env APP_ENV=staging pnpm run android",
    "ios:staging": "cross-env APP_ENV=staging pnpm run ios"
  },
  "dependencies": {
    "@bacons/text-decoder": "^0.0.0",
    "@clerk/clerk-expo": "^1.1.4",
    "@expo/metro-config": "^0.18.3",
    "@react-native-community/datetimepicker": "8.0.1",
    "@react-native-picker/picker": "2.7.5",
    "@shopify/flash-list": "1.6.4",
    "@tanstack/react-query": "^5.35.5",
    "@trpc/client": "11.0.0-rc.364",
    "@trpc/react-query": "11.0.0-rc.364",
    "@trpc/server": "11.0.0-rc.364",
    "clsx": "^2.1.1",
    "expo": "~51.0.8",
    "expo-auth-session": "^5.5.2",
    "expo-constants": "~16.0.1",
    "expo-crypto": "^13.0.2",
    "expo-dev-client": "~4.0.14",
    "expo-font": "^12.0.5",
    "expo-linking": "~6.3.1",
    "expo-random": "~14.0.1",
    "expo-router": "~3.5.14",
    "expo-secure-store": "^13.0.1",
    "expo-splash-screen": "~0.27.4",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.4",
    "nativewind": "~4.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "~0.74.1",
    "react-native-css-interop": "~0.0.34",
    "react-native-date-picker": "^5.0.4",
    "react-native-gesture-handler": "~2.16.2",
    "react-native-modal-datetime-picker": "^17.1.0",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "~4.10.1",
    "react-native-screens": "~3.31.1",
    "react-native-select-dropdown": "^4.0.1",
    "superjson": "2.2.1",
    "tailwind-merge": "^2.3.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/runtime": "^7.24.5",
    "@planty/api": "workspace:*",
    "@planty/eslint-config": "workspace:*",
    "@planty/prettier-config": "workspace:*",
    "@planty/tailwind-config": "workspace:*",
    "@planty/tsconfig": "workspace:*",
    "@types/babel__core": "^7.20.5",
    "@types/react": "^18.2.79",
    "cross-env": "^7.0.3",
    "eslint": "^9.2.0",
    "jest": "^29.7.0",
    "jest-expo": "^51.0.2",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.3.3"
  },
  "prettier": "@planty/prettier-config",
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(?:.pnpm/)?((jest-)?react-native|@react-native(-community)?|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg))"
    ]
  }
}
